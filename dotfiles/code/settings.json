{
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "python.languageServer": "Pylance",
    "python.analysis.typeCheckingMode": "basic",
    "editor.insertSpaces": false,
    "workbench.colorTheme": "Solarized Light",
    "markdown-preview-enhanced.codeBlockTheme": "solarized-light.css",
    "python.defaultInterpreterPath": "/usr/bin/python3",
    "python.formatting.provider": "yapf",
    "python.formatting.yapfArgs": [
        "--style",
        "{based_on_style: pep8, use_tabs: true, continuation_align_style: 'valign-right', coalesce_brackets: true, dedent_closing_brackets: true, SPLIT_COMPLEX_COMPREHENSION: true, SPLIT_BEFORE_EXPRESSION_AFTER_OPENING_PAREN: true, column_limit: 128, spaces_before_comment: 1}"
    ],
    "python.formatting.yapfPath": "/usr/local/bin/yapf",
    "explorer.confirmDelete": false,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "rust-analyzer.diagnostics.disabled": [
        "missing-unsafe"
    ],
    "window.zoomLevel": 2,
    "security.workspace.trust.untrustedFiles": "open",
    "files.autoSave": "afterDelay",
    "editor.formatOnSave": true,
}